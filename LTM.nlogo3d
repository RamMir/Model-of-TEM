  if (in_plaque? = true)
  [if (T_count > T)
    [die]
  ]
end

to produce-macrophage

  ask monocytes [
    set c count monocytes with [in_plaque? = true]
;    show (word" number of monocytes in plaque:" c)
  ]

  ask n-of ( 2 * c / 3 ) monocytes with [in_plaque? = true]
   [ if (T_count > T)
      [
        ask patch-here [
          sprout-macro1s 1 [set color blue                                                                                           ; Macrophage production
            set shape "circle"
            set T_count 0
            set T Tm1]
          set inPlaque? true
          ]
  ;show (word " M1 generation complete")
  die
  ]
   ]
ask monocytes with [in_plaque? = true]
[ if (T_count > T)
  [
  ask patch-here [
    sprout-macro2s 1 [set color yellow
      set shape "circle"
      set T_count 0
      set T Tm2]
    set inPlaque? true
  ]
  die
]
]

end

to produce-foamCell
  if (T_count > T)
  [ ask patch-here [
      if ( ox-ldl > 100 * 10 ^ (-9))[                                                                                                  ; Foam cell production (Dubland, J.A.)
    sprout-FCs 1[ set color red
      set shape "cloud"
      set size 0.7]
    set inPlaque? true
    set ox-ldl ox-ldl - 100 * 10 ^ (-9)
  ]
  ]
  die
  ]
