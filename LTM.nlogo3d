to LDL_infiltration

ask ECs with [(count neighbors with [islumen? = true and ldl > 0]) > 0]
[
ask patch-here[
set ldl_total 0
set count_neigh count neighbors with [islumen? = true]
  ask neighbors with [islumen? = true]                                                                     ; LDL infiltration through EC
  [
    let ldl_funct (2e-08 * WSS_int ^ 6 - 2E-06 * WSS_int ^ 5 + 6E-05 * WSS_int ^ 4 - 0.0011 * WSS_int ^ 3 + 0.0104 * WSS_int ^ 2 - 0.0509 * WSS_int + 1.145)
    set ldl_total (0.7 * ldl * ldl_funct) + ldl_total
    set ldl ldl - (0.7 * ldl * ldl_funct)
   ; show ldl_total
      ]
  set ldl ldl_total / count_neigh
  ;show (word "ldl on EC: "ldl_total)
  ]
]

end


to get_WSS

  let xint pxcor * Unitfactor
  let yint pycor * Unitfactor
  let zint pzcor * Unitfactor
  let pos (list xint yint zint )

  matlab:eval "a=1;"
  matlab:send-double-list "sendPos" pos
  matlab:eval "get_WSS_netlogo(sendPos);a=a+1;"
  set WSS_int matlab:get-double "retWSS"

   show WSS_int


  ; Don't continue until get_WSS_netlogo() is done                                                                                       ; Getting WSS from CFD
  set matlabReady? false
  let mlcount 0
  while [ matlabReady? = false ]
  [
    set matlabReturnVal (matlab:get-double "a")
    if matlabReturnVal = 2
    [
      set matlabReady? true
    ]
    if mlcount > 50
    [
      set matlabReady? true
    ]
    set mlcount (mlcount + 1)
    wait 0.1
  ]

  ; show mlcount

end

