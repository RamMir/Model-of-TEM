to update_cytokine

  set tnf tnf + (( Cn * tnf_perneut ) + (Cl * tnf_perlympho) + (Cm * tnf_permono) + (Cm1 * tnf_perm1))
  set il1 il1 + (( Cn * il1_perneut ) + (Cl * il1_perlympho) + (Cm * il1_permono) + (Cm1 * il1_perm1))
  set il10 il10 + (( Cn * il10_perneut ) + (Cl * il10_perlympho) + (Cm * il10_permono) + (Cm1 * il10_perm1))

end

to get_adhesion_probability

;    show (word " number of neutrophils here:" count neutrophils-here)
;    show (word " number of monocytes here:" count monocytes-here)
;    show (word " number of lymphocytes here:" count lymphocytes-here)

;    set threshold_vicinity_mm threshold_vicinity * 0.1                                                                               ; 1 Unit = 100 um = 0.1 mm

   set pTNFalpha_neutro 0
   set pIL1_neutro 0

   set pTNFalpha_mono 0
   set pIL1_mono 0

   set pTNFalpha_lympho 0
   set pIL1_lympho 0

   set count_ECs count neighbors with [(count ECs-here) > 0 and ( 1.42 > (distance myself) )]
  ; set count_ECs count neighbors with [(count ECs-here) > 0 ]                                                                                                  ; Finding cytokines from neighbors
   ;show (word " Closest neighbors:" count_ECs)
   ask neighbors with [(count ECs-here) > 0 and ( 1.42 > (distance myself) ) ]
   ;ask neighbors with [(count ECs-here) > 0]
   [
      ;show (word " tnf at closest neighbors:" tnf)
      ;show (word " il1 at closest neighbors:" il1)

      set threshold_vicinity_mm distance myself * 0.1                                                                               ; 1 Unit = 100 um = 0.1 mm
      ;show (word " distance to EC:" threshold_vicinity_mm)

       set TNF-alpha tnf
       set IL-1 il1

 ; Compute probability for neutrophil adhesion with cytokine

          ifelse (TNF-alpha <= 1)
   [set pTNFalpha_indv_neutro 0.0996 * TNF-alpha
   ]
   [ let TNFlog (log TNF-alpha 10)
     set pTNFalpha_indv_neutro ( Tadhesion / TobsTNF_neutro ) * ( 1 / threshold_vicinity_mm ) * ( 0.80024 - 0.68069 * ( 1 - exp ( - 0.32533 * TNFlog ^ ( - 0.56225 ))))           ;Neutrophil adhesion Vs TNF (Bahra)
    ]
  ; show (word " individual probability of adhesion due to tnf:" pTNFalpha_indv_neutro)
   set pTNFalpha_neutro pTNFalpha_neutro + pTNFalpha_indv_neutro

   ifelse (IL-1 <= 0)
   [set pIL1_indv_neutro 0
   ]
  [ set pIL1_indv_neutro ( Tadhesion / TobsIL1_neutro ) * ( 1 / threshold_vicinity_mm ) * ( 0.10423 + 0.18626 * ( 1 - exp ( - 28.2949 * IL-1 ^ 1.4569 ) ) )                       ; Neutrophil adhesion Vs IL1 (Breviario)
    ]
   ;show ( word "Individual  Adhesion probability vs. IL-1: " pIL1_indv_neutro )
   set pIl1_neutro pIl1_neutro + pIL1_indv_neutro

   ;show pTNFalpha_neutro
   ;show pIl1_neutro

 ; Compute probability for monocyte adhesion with cytokine

             ifelse (TNF-alpha <= 1)
   [set pTNFalpha_indv_neutro 0.0996 * TNF-alpha
   ]
   [ let TNFlog (log TNF-alpha 10)
     set pTNFalpha_indv_neutro ( 0.80024 - 0.68069 * ( 1 - exp ( - 0.32533 * TNFlog ^ ( - 0.56225 ))))           ;Neutrophil adhesion Vs TNF (Bahra)
    ]
   set pTNFalpha_indv_neutro ( Tadhesion / TobsTNF_neutro ) * ( 1 / threshold_vicinity_mm ) * pTNFalpha_indv_neutro
  ; show (word " individual probability of adhesion due to tnf:" pTNFalpha_indv_neutro)
   set pTNFalpha_neutro pTNFalpha_neutro + pTNFalpha_indv_neutro

   ifelse (IL-1 <= 0)
   [set pIL1_indv_neutro 0
   ]
  [ set pIL1_indv_neutro ( 0.10423 + 0.18626 * ( 1 - exp ( - 28.2949 * IL-1 ^ 1.4569 ) ) )                       ; Neutrophil adhesion Vs IL1 (Breviario)
    ]
  set pIL1_indv_neutro ( Tadhesion / TobsIL1_neutro ) * ( 1 / threshold_vicinity_mm ) * pIL1_indv_neutro
   ;show ( word "Individual  Adhesion probability vs. IL-1: " pIL1_indv_neutro )
   set pIl1_neutro pIl1_neutro + pIL1_indv_neutro

   ;show pTNFalpha_neutro
   ;show pIl1_neutro

 ; Compute probability for monocyte adhesion with cytokine

      ifelse (TNF-alpha <= 1)
   [set pTNFalpha_indv_mono 0.0996 * TNF-alpha
   ]
   [ let TNFlog (log TNF-alpha 10)
     set pTNFalpha_indv_mono ( 0.80024 - 0.68069 * ( 1 - exp ( - 0.32533 * TNFlog ^ ( - 0.56225 ))))                  ; Monocyte adhesion vs TNF
    ]
   set pTNFalpha_indv_mono ( Tadhesion / TobsTNF_mono ) * ( 1 / threshold_vicinity_mm ) * pTNFalpha_indv_mono
;   show (word " individual probability of adhesion due to tnf:" pTNFalpha_indv_mono)
   set pTNFalpha_mono pTNFalpha_mono + pTNFalpha_indv_mono

   ifelse (IL-1 <= 0)
   [set pIL1_indv_mono 0
   ]
  [ set pIL1_indv_mono ( 0.144 + 0.803 * ( 1 - exp ( - 0.58154 * IL-1 ^ 1.1875 ) ) )                                ; Monocyte adhesion vs IL-1 (Bevilacqua)
    ]
  set pIL1_indv_mono ( Tadhesion / TobsIL1_mono ) * ( 1 / threshold_vicinity_mm ) * pIL1_indv_mono
;   show ( word "Individual Monocyte Adhesion probability vs. IL-1: " pIL1_indv_mono )
   set pIl1_mono pIl1_mono + pIL1_indv_mono

   ;show pTNFalpha_mono
   ;show pIl1_mono

 ; Compute probability for lymphocyte adhesion with cytokine

    ifelse (TNF-alpha <= 1)
   [set pTNFalpha_indv_lympho 0.0996 * TNF-alpha
   ]
   [ let TNFlog (log TNF-alpha 10)
     set pTNFalpha_indv_lympho ( 0.80024 - 0.68069 * ( 1 - exp ( - 0.32533 * TNFlog ^ ( - 0.56225 ))))                             ; Lymphocyte adhesion vs TNF
    ]
   set pTNFalpha_indv_lympho ( Tadhesion / TobsTNF_lympho ) * ( 1 / threshold_vicinity_mm ) * pTNFalpha_indv_lympho
   ;show (word " individual probability of adhesion due to tnf:" pTNFalpha_indv_lympho)
   set pTNFalpha_lympho pTNFalpha_lympho + pTNFalpha_indv_lympho

   ifelse (IL-1 <= 0)
   [set pIL1_indv_lympho 0
   ]
  [ set pIL1_indv_lympho ( 0.10423 + 0.18626 * ( 1 - exp ( - 28.2949 * IL-1 ^ 1.4569 ) ) )                                         ; Lymphocyte adhesion vs IL-1
    ]
  set pIL1_indv_lympho ( Tadhesion / TobsIL1_lympho ) * ( 1 / threshold_vicinity_mm ) * pIL1_indv_lympho 
   ;show ( word "Individual  Adhesion probability vs. IL-1: " pIL1_indv_lympho )
   set pIl1_lympho pIl1_lympho + pIL1_indv_lympho

  ; show pTNFalpha_lympho
  ; show pIl1_lympho
   ]


   set pTNFalpha_neutro pTNFalpha_neutro / count_ECs
   set pIl1_neutro pIl1_neutro / count_ECs

   set pTNFalpha_mono pTNFalpha_mono / count_ECs
   set pIl1_mono pIl1_mono / count_ECs

   set pTNFalpha_lympho pTNFalpha_lympho / count_ECs
   set pIl1_lympho pIl1_lympho / count_ECs


 ; Compute probability  with WSS
;  show (word "WSS " WSS_int ", x " pxcor ", y " pycor ", z " pzcor)
        set WSS_int abs WSS_int
        let WSS_dyncm 10 * WSS_int

 ; Compute probability for neutrophil adhesion with WSS

        let pWSS_neutro 0
        ifelse ( WSS_dyncm < 12 )
        [ set pWSS_neutro ( Tadhesion / TobsWSS_neutro ) * ( 1 / 0.1) * ( 2.6e-3 * WSS_dyncm ^ 2 - 0.0828 * WSS_dyncm  + 0.6343 ) ]
        [ set pWSS_neutro 0]
 ;  show ( word "Neutrophil Adhesion probability vs. WSS: " pWSS_neutro )

 ; Compute probability for monocyte adhesion with WSS

    let pWSS_mono 0
        ifelse ( WSS_dyncm < 10 )
        [ set pWSS_mono ( Tadhesion / TobsWSS_mono ) * ( 1 / 0.1) * ( - 0.0003 * WSS_dyncm ^ 3 + 0.013811 * WSS_dyncm ^ 2 - 0.1839 * WSS_dyncm + 0.791 ) ]                                                  ; Monocyte adhesion vs WSS
        [ set pWSS_mono 0]
;  show ( word "monocyte Adhesion probability vs. WSS: " pWSS_mono )

 ; Compute probability for lymphocyte adhesion with WSS

    let pWSS_lympho 0
    ifelse ( WSS_dyncm < 4.1 )
    [ set pWSS_lympho ( Tadhesion / TobsWSS_lympho ) * ( 1 / 0.1 ) * ( - 9.99325e-3 * WSS_dyncm ^ 3 + 0.10811 * WSS_dyncm ^ 2 - 0.40969 * WSS_dyncm + 0.54938 ) ]                                                ; Lymphocyte adhesion vs WSS
    [ set pWSS_lympho 0]
;  show ( word "lymphocyte Adhesion probability vs. WSS: " pWSS_lympho )


; Compute total probability of neutrophil

    set pAdhesion_neutro (1 / 15) * ( pTNFalpha_neutro + pIL1_neutro + pWSS_neutro )
;    show (word " Total neutrophil adhesion probability:" pAdhesion_neutro)

    if ( pAdhesion_neutro > 1) [ set pAdhesion_neutro 1 ]

    let pMigration_neutro ( pStiff_neutro )

    let pTEM_neutro ( pAdhesion_neutro * pMigration_neutro )
;    show (word "probability of transmigration of neutrophil:" pTEM_neutro)


    ask neutrophils-here [
    set pRand random-float 1
        ;show (word "random number generated:"pRand)
        set pTEM pTEM_neutro
        ;show (word "pTEM of neutrophil:" pTEM)
    ]

  let  neutro count neutrophils-here with [pRand < pTEM]
;  show (word" number of neutrophils that transmigrate:" neutro)


; Compute total probability of monocyte

  set pAdhesion_mono (1 / 15) * (pTNFalpha_mono + pIL1_mono + pWSS_mono)
;  show (word " Total monocyte adhesion probability:" pAdhesion_mono)

    if ( pAdhesion_mono > 1) [ set pAdhesion_mono 1 ]

        let pMigration_mono ( pStiff_mono )

        set pTEM_mono ( pAdhesion_mono * pMigration_mono )
 ;       show (word "probability of transmigration of monocyte:" pTEM_mono)

        ask monocytes-here [
        set pRand random-float 1
        ;show (word "random number generated:"pRand)
        set pTEM pTEM_mono
        ;show pTEM
  ]

  let  mono count monocytes-here with [pRand < pTEM]
  ;show (word" number of monocytes that transmigrate:" mono)

; Compute total probability of lymphocyte

  set pAdhesion_lympho (1 / 15) * (pTNFalpha_lympho + pIL1_lympho + pWSS_lympho)
;  show (word " Total lymphocyte adhesion probability:" pAdhesion_lympho)

    if ( pAdhesion_lympho > 1) [ set pAdhesion_lympho 1 ]

        let pMigration_lympho ( pStiff_lympho )

        let pTEM_lympho ( pAdhesion_lympho * pMigration_lympho )
  ;      show (word "probability of transmigration of lymphocyte:" pTEM_lympho)


   ask lymphocytes-here [
    set pRand random-float 1
        ;show (word "random number generated:"pRand)
        set pTEM pTEM_lympho
        ;show pTEM
        ]

  let  lympho count lymphocytes-here with [pRand < pTEM]
;  show (word" number of lymphocytes that transmigrate:" lympho)


  set leuko neutro + mono + lympho
  if (leuko > 0)
  [
  ask one-of turtles-here with [pRand < pTEM]
  [make-plaque]
  ]

;  ask neutrophils-here  with [pRand < pTEM_neutro][
;    move-to patch x_final y_final z_final
;    show xcor
;    set in_plaque?  true
;    ask patch-here [show (word " final position:"pxcor)]                                                                                                                            ; TRANSMIGRATION
;  ]
;   ask monocytes-here  with [pRand < pTEM_mono][
;    move-to patch x_final y_final z_final
;    show xcor
;    set in_plaque?  true
;    ask patch-here [show (word " final position:"pxcor)]
;  ]
;    ask lymphocytes-here  with [pRand < pTEM_lympho][
;    move-to patch x_final y_final z_final
;    show xcor
;    set in_plaque?  true
;    ask patch-here [show (word " final position:"pxcor)]
;  ]
 if(IL-1 > 0 or TNF-alpha > 0)
 [ask turtles-here  with [pRand < pTEM][
    move-to patch x_final y_final z_final
    ;show xcor
    set in_plaque?  true
    ask patch-here [show (word " final position:"pxcor)]                                                                                                                            ; TRANSMIGRATION
  ]
 ]
